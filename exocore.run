export = {
  "command": "command -v python3 >/dev/null || (apt-get update && apt-get install -y python3) && " +
             "command -v pip >/dev/null || apt-get install -y python3-pip && " +
             "pip install --break-system-packages -r requirements.txt"
}

function = {
  run = "python3 index.py"
}